<project name="plagiatsjaeger-app" default="build" basedir="..">

	<property name="webroot" value="../../../../../../srv/test" />
	<property name="source" value="*" />
	<property name="deldir" value="/srv/www/htdocs"/>
	<target name="deploy" depends="build">
		<echo message="Start deploy"/>
		<delete dir="${deldir}" />
		<copy todir="${webroot}/${project}">
			<fileset dir="${destdir}" />
		</copy>
		<echo message="Finish deploy"/>
	</target>
	<target name="build" />
</project>